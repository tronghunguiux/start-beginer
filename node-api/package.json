{
  "name": "node-api",
  "version": "1.0.0",
  "privte": true,
  "description": "beginner node backend api with nodejs",
  "engines": {
    "node": ">=14.17.0"
  },
  "main": "src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "main": "node src/server.js",
    "eslint": "eslint src --ext js report-unused-disable-directives --max-warnings=0",
    "clean": "rm -rf dist && mkdir dist",
    "build:babel": "babel ./src -d ./dist",
    "build": "npm run clean && npm run build:babel",
    "production": "npm run build && node ./dist/server.js",
    "dev": "nodemon --exec babel-node ./src/server.js"
  },
  "keywords": [
    "nodejs",
    "api",
    "backend-api"
  ],
  "author": "tronghung.uiux",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.26.10",
    "dotenv": "^16.4.7",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.26.10",
    "@babel/node": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.23.0",
    "nodemon": "^3.1.9"
  }
}
